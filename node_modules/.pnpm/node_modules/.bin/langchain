#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/akshay/Projects/repos/node-langchain-pinecone/node_modules/.pnpm/langchainplus-sdk@0.0.19/node_modules/langchainplus-sdk/dist/cli/node_modules:/home/akshay/Projects/repos/node-langchain-pinecone/node_modules/.pnpm/langchainplus-sdk@0.0.19/node_modules/langchainplus-sdk/dist/node_modules:/home/akshay/Projects/repos/node-langchain-pinecone/node_modules/.pnpm/langchainplus-sdk@0.0.19/node_modules/langchainplus-sdk/node_modules:/home/akshay/Projects/repos/node-langchain-pinecone/node_modules/.pnpm/langchainplus-sdk@0.0.19/node_modules:/home/akshay/Projects/repos/node-langchain-pinecone/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/akshay/Projects/repos/node-langchain-pinecone/node_modules/.pnpm/langchainplus-sdk@0.0.19/node_modules/langchainplus-sdk/dist/cli/node_modules:/home/akshay/Projects/repos/node-langchain-pinecone/node_modules/.pnpm/langchainplus-sdk@0.0.19/node_modules/langchainplus-sdk/dist/node_modules:/home/akshay/Projects/repos/node-langchain-pinecone/node_modules/.pnpm/langchainplus-sdk@0.0.19/node_modules/langchainplus-sdk/node_modules:/home/akshay/Projects/repos/node-langchain-pinecone/node_modules/.pnpm/langchainplus-sdk@0.0.19/node_modules:/home/akshay/Projects/repos/node-langchain-pinecone/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../langchainplus-sdk/dist/cli/main.cjs" "$@"
else
  exec node  "$basedir/../langchainplus-sdk/dist/cli/main.cjs" "$@"
fi
